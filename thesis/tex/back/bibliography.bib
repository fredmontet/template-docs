	Doi = {10.1016/S0140-6736(05)17741-1},
	Journal = {Lancet},
	Journal-Full = {Lancet (London, England)},
	Mesh = {Adolescent; Adult; Aged; Female; Global Health; Humans; Hypertension; Male; Middle Aged; Prevalence},
