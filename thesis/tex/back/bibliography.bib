	Mesh = {Adult; Blood Pressure; Cardiovascular Diseases; Female; Follow-Up Studies; Humans; Hypertension; Kidney Glomerulus; Male; Middle Aged; Predictive Value of Tests; Prognosis; Proportional Hazards Models; Retrospective Studies; Sodium, Dietary},
	Month = {Dec},
